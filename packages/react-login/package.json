  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build"
  },
  "dependencies": {
    "@telegram-widgets/login": "workspace:^",
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0"
  },
  "peerDependencies": {
    "next": ">=13.1.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  }

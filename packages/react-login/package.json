{
  "name": "@telegram-widgets/react-login",
  "version": "0.0.0",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./middleware": {
      "import": "./dist/middleware.es.js",
      "require": "./dist/middleware.cjs.js"
    }
  },
  "typesVersions": {
